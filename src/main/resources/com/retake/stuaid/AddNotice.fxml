<AnchorPane fx:id="rootAddNotice" accessibleRole="DIALOG" prefHeight="310.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.retake.stuaid.AddNoticeController">
   <children>
      <Button fx:id="btnNoticeSubmit" layoutX="284.0" layoutY="212.0" mnemonicParsing="false" onAction="#addNewNotice" style="-fx-cursor: hand;" styleClass="btnGreen" stylesheets="@css/BtnStyle.css" text="Submit" />
      <Button fx:id="btnNoticeCancel" layoutX="207.0" layoutY="212.0" mnemonicParsing="false" onAction="#cancelNoticeStage" style="-fx-cursor: hand;" styleClass="btnRed" stylesheets="@css/BtnStyle.css" text="Cancel" />
      <Label layoutX="151.0" layoutY="4.0" prefHeight="30.0" prefWidth="106.0" text="Add Notice">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextArea fx:id="txtRefArea" layoutX="56.0" layoutY="41.0" onDragDetected="#addNewRef" prefHeight="154.0" prefWidth="284.0" promptText="New Notice" style="-fx-cursor: text;" />
   </children>
</AnchorPane>
